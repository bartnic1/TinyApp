<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_header %>
  <style>
  table {
    border-collapse: collapse;
    border-spacing: 0px;
  }
  table, th, td {
    padding: 5px;
    border: 1px solid black;
  }
  </style>
</head>
<body>
    <!-- Add your EJS code here -->
    <table>
      <tr>
        <th>Short URL</th>
        <th>Long URL</th>
        <th>Delete URL</th>
      </tr>
      <%for (var key in entries){ %>
      <tr>
        <td><%=key %></td>
        <td><%=entries[key] %></td>
        <td>
        <form method="POST" action="/urls/<%=key%>/delete">
        <input type="submit" value="Delete" name="<%=key%>" style="margin-left: 15px">
        </form>
        </td>
      </tr>
      <%}%>
    </table>
    </br>
    <a href="">Link to shorten URL</a>
    <!-- <form method="POST" action="/urls/9sm5xK/delete">
      <button
    </form>
     -->
</body>
</html>